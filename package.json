{
  "name": "morose",
  "version": "0.3.11",
  "description": "npm registry & proxy server",
  "bin": {
    "morose": "./dist/index.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "build:watch": "tsc -w",
    "prebuild": "npm run clean",
    "prepublish": "npm run build:prod",
    "compile": "tsc && node ./dist/index.js",
    "dev": "nodemon -w src -e ts --exec \"npm run compile\"",
    "build:prod": "npm run build && npm run build:app:prod",
    "build:app": "webpack --env.dev --progress --hide-modules",
    "build:app:prod": "webpack --env.aot -p --progress --hide-modules",
    "start": "webpack-dev-server --env.dev --env.serve --progress",
    "start:aot": "webpack-dev-server --env.aot --env.dev --env.serve --progress",
    "test": "node ./tests/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bleenco/morose.git"
  },
  "keywords": [
    "npm",
    "package",
    "registry"
  ],
  "authors": [
    "Jan Kuri <jkuri88@gmail.com>",
    "Irman Abdic <irman.abdic@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bleenco/morose/issues"
  },
  "homepage": "https://github.com/bleenco/morose#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "chalk": "^1.1.3",
    "cors": "^2.8.1",
    "express": "^4.15.2",
    "fs-extra": "^2.1.2",
    "glob": "^7.1.1",
    "request": "^2.81.0",
    "semver": "^5.3.0",
    "semver-extra": "^2.0.1"
  },
  "devDependencies": {
    "@angular/animations": "4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngtools/webpack": "^1.2.14",
    "@types/body-parser": "1.16.1",
    "@types/chai": "^3.4.35",
    "@types/chai-http": "^0.0.30",
    "@types/chalk": "^0.4.31",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.35",
    "@types/fs-extra": "^2.0.0",
    "@types/glob": "^5.0.30",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.8",
    "@types/request": "^0.0.41",
    "@types/semver": "^5.3.31",
    "add-asset-html-webpack-plugin": "^1.0.2",
    "bulma": "^0.4.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.27.3",
    "d3": "^1.3.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.1",
    "nodemon": "^1.11.0",
    "portfinder": "^1.0.13",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "rxjs": "^5.0.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.0",
    "typescript": "^2.2.1",
    "typicons-font": "^1.0.0",
    "webpack": "^2.3.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-merge": "^4.1.0",
    "zone.js": "^0.8.4"
  }
}
