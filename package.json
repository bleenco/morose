{
  "name": "morose",
  "version": "0.8.0",
  "description": "npm registry & proxy server",
  "bin": {
    "morose": "./dist/api/server.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "webpack --config webpack.server.js --progress --hide-modules",
    "build:watch": "tsc -w",
    "compile": "tsc && node ./dist/api/index.js",
    "dev": "nodemon -w src/api -i src/app -e ts --exec \"npm run compile\" & npm start",
    "prebuild:prod": "npm run clean",
    "build:prod": "npm run build & npm run build:app:prod",
    "build:app": "webpack --env.dev --progress --hide-modules",
    "build:app:prod": "webpack --env.aot -p --progress --hide-modules",
    "start": "webpack-dev-server --env.dev --env.serve --progress",
    "start:aot": "webpack-dev-server --env.aot --env.dev --env.serve --progress",
    "test:unit": "node ./tests/run.js",
    "test:e2e": "node ./tests/run_e2e.js",
    "test:protractor": "npm run e2e",
    "lint": "tslint ./src/api/**/*.ts ./src/app/**/*.ts ./tests/**/*.ts",
    "protractor:live": "npm run e2e -- --elementExplorer",
    "protractor": "protractor ./protractor.conf.js",
    "protractor:delay": "sleep 30 && protractor ./protractor.conf.js",
    "protractor:ci": "npm run dev & npm run protractor:delay",
    "webdriver": "npm run webdriver:update -- --standalone",
    "preprotractor": "npm run webdriver",
    "preprotractor:ci": "npm run webdriver",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bleenco/morose.git"
  },
  "keywords": [
    "npm",
    "package",
    "registry"
  ],
  "authors": [
    "Jan Kuri <jkuri88@gmail.com>",
    "Irman Abdic <irman.abdic@gmail.com>",
    "Izak Lipnik <izak.lipnik@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bleenco/morose/issues"
  },
  "homepage": "https://github.com/bleenco/morose#readme",
  "dependencies": {},
