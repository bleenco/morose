{
  "name": "morose",
  "version": "0.4.0",
  "description": "npm registry & proxy server",
  "bin": {
    "morose": "./dist/api/index.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "build:watch": "tsc -w",
    "prebuild": "npm run clean",
    "compile": "npm run lint && tsc && node ./dist/api/index.js",
    "dev": "nodemon -w src/api -i src/app -e ts --exec \"npm run compile\"",
    "build:prod": "npm run build && npm run build:app:prod",
    "build:app": "webpack --env.dev --progress --hide-modules",
    "build:app:prod": "webpack --env.aot -p --progress --hide-modules",
    "start": "webpack-dev-server --env.dev --env.serve --progress",
    "start:aot": "webpack-dev-server --env.aot --env.dev --env.serve --progress",
    "test": "npm-run-all -c test:unit test:e2e",
    "test:unit": "node ./tests/run.js",
    "test:e2e": "node ./tests/run_e2e.js",
    "lint": "tslint ./src/api/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bleenco/morose.git"
  },
  "keywords": [
    "npm",
    "package",
    "registry"
  ],
  "authors": [
    "Jan Kuri <jkuri88@gmail.com>",
    "Irman Abdic <irman.abdic@gmail.com>",
    "Izak Lipnik <izak.lipnik@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bleenco/morose/issues"
  },
  "homepage": "https://github.com/bleenco/morose#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "chalk": "^1.1.3",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.1",
    "jsonwebtoken": "^7.4.1",
    "request": "^2.81.0",
    "semver": "^5.3.0",
    "semver-extra": "^2.0.1",
    "ws": "^3.0.0"
  },
  "devDependencies": {
    "@angular/animations": "4.1.3",
    "@angular/common": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/compiler-cli": "4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/forms": "^4.1.3",
    "@angular/http": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@angular/platform-server": "^4.1.3",
    "@angular/router": "^4.1.3",
    "@ngtools/webpack": "^1.3.1",
    "@types/body-parser": "1.16.3",
    "@types/chai": "^3.5.2",
    "@types/chalk": "^0.4.31",
    "@types/cors": "^2.8.1",
    "@types/d3": "^4.8.0",
    "@types/express": "^4.0.35",
    "@types/fs-extra": "^3.0.1",
    "@types/glob": "^5.0.30",
    "@types/jsonwebtoken": "^7.2.0",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.18",
    "@types/request": "^0.0.43",
    "@types/semver": "^5.3.31",
    "@types/ws": "^0.0.41",
    "add-asset-html-webpack-plugin": "^2.0.1",
    "angular2-jwt": "^0.2.3",
    "bulma": "^0.4.1",
    "chai": "^3.5.0",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.1",
    "d3": "4.9.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "minimist": "^1.2.0",
    "mocha": "^3.4.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "portfinder": "^1.0.13",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "rxjs": "^5.4.0",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "suppose": "^0.6.2",
    "temp": "^0.8.3",
    "tree-kill": "^1.1.0",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2",
    "typicons-font": "^1.0.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
